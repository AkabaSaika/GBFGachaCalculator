{
    "manifest_version": 3,
    "name": "Granblue Fantasy Helper",
    "version": "1.0",
    "permissions": ["storage","activeTab", "scripting", "tabs", "https://*/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }